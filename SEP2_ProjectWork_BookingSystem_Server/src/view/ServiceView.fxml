<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="220.0" prefWidth="375.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ServiceViewController">
   <children>
      <VBox prefHeight="183.0" prefWidth="366.0">
         <children>
            <HBox prefHeight="42.0" prefWidth="366.0">
               <children>
                  <VBox prefHeight="51.0" prefWidth="30.0" />
                  <Label prefHeight="53.0" prefWidth="170.0" text="Service">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="38.0" prefWidth="366.0">
               <children>
                  <VBox prefHeight="126.0" prefWidth="28.0" />
                  <Label prefHeight="38.0" prefWidth="96.0" text="Service type : ">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="serviceTypeText" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="38.0" prefWidth="366.0">
               <children>
                  <VBox prefHeight="126.0" prefWidth="28.0" />
                  <Label prefHeight="38.0" prefWidth="96.0" text="Price : ">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="priceText" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="54.0" prefWidth="366.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="54.0" prefWidth="232.0">
                     <children>
                        <Label fx:id="errorLabel" alignment="CENTER" prefHeight="54.0" prefWidth="202.0" text="error" textFill="RED" wrapText="true">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                     </children></VBox>
                  <Button mnemonicParsing="false" onAction="#onSaveBtn" prefHeight="38.0" prefWidth="90.0" text="SAVE" textFill="#3a8ae8">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
